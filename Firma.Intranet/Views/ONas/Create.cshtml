@model Firma.Data.Data.CMS.ONas

@{
    ViewData["Title"] = "Create";
}

<section class="section section-details grey lighten-4">
    <div class="container">
        <div class="row">
            <div class="col s12">
                <div class="card">
                    <div class="card-content">
                        <div class="row">
                            <div class="col s12 m6">
                                <span class="card-title">Dodaj treść strony "O nas"</span>
                            </div>
                            
                        </div>
                        <form asp-action="Create">
                            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                            <div class="form-group">
                                <label asp-for="Naglowek" class="control-label"></label>
                                <input asp-for="Naglowek" class="form-control" />
                                <span asp-validation-for="Naglowek" class="text-danger"></span>
                            </div>
                            <div class="form-group">
                                <label asp-for="Tekst" class="control-label"></label>
                                <input asp-for="Tekst" class="form-control" />
                                <span asp-validation-for="Tekst" class="text-danger"></span>
                            </div>
                            <div class="form-group">
                                <label asp-for="Historia" class="control-label"></label>
                                <input asp-for="Historia" class="form-control" />
                                <span asp-validation-for="Historia" class="text-danger"></span>
                            </div>
                            <div class="form-group">
                                <label asp-for="Zdjecia" class="control-label"></label>
                                <input asp-for="Zdjecia" class="form-control" />
                                <span asp-validation-for="Zdjecia" class="text-danger"></span>
                            </div>
                            <div class="form-group">
                                <label asp-for="Kontakt" class="control-label"></label>
                                <input asp-for="Kontakt" class="form-control" />
                                <span asp-validation-for="Kontakt" class="text-danger"></span>
                            </div>
                            <div class="form-group">
                                <input type="submit" value="Create" class="btn btn-primary" />
                                <a asp-action="Index"><input type="button" value="Powrót" class="btn btn-primary" /></a>
                            </div>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}





